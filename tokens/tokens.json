{
  "core": {
    "dimension": {
      "scale": {
        "$type": "dimension",
        "$value": "2"
      },
      "xs": {
        "$type": "dimension",
        "$value": "4"
      },
      "sm": {
        "$type": "dimension",
        "$value": "{dimension.xs} * {dimension.scale}"
      },
      "md": {
        "$type": "dimension",
        "$value": "{dimension.sm} * {dimension.scale}"
      },
      "lg": {
        "$type": "dimension",
        "$value": "{dimension.md} * {dimension.scale}"
      }
    },
    "spacing": {
      "xs": {
        "$type": "spacing",
        "$value": "{dimension.xs}"
      },
      "sm": {
        "$type": "spacing",
        "$value": "{dimension.sm}"
      },
      "md": {
        "$type": "spacing",
        "$value": "{dimension.md}"
      },
      "lg": {
        "$type": "spacing",
        "$value": "{dimension.lg}"
      }
    },
    "borderRadius": {
      "none": {
        "$type": "borderRadius",
        "$value": "0"
      },
      "sm": {
        "$type": "borderRadius",
        "$value": "4"
      }
    },
    "colors": {
      "black": {
        "$type": "color",
        "$value": "#000000"
      },
      "white": {
        "$type": "color",
        "$value": "#ffffff"
      },
      "zinc": {
        "600": {
          "$type": "color",
          "$value": "#d1d5db"
        },
        "900": {
          "$type": "color",
          "$value": "#18181b"
        }
      },
      "teal": {
        "300": {
          "$type": "color",
          "$value": "#2dd4bf4d"
        },
        "600": {
          "$type": "color",
          "$value": "#14b8a6"
        }
      }
    },
    "opacity": {
      "none": {
        "$type": "opacity",
        "$value": "0%"
      },
      "low": {
        "$type": "opacity",
        "$value": "10%"
      },
      "max": {
        "$type": "opacity",
        "$value": "100%"
      }
    },
    "fontFamilies": {
      "heading": {
        "$type": "fontFamilies",
        "$value": "Inter"
      },
      "body": {
        "$type": "fontFamilies",
        "$value": "Inter"
      },
      "code": {
        "$type": "fontFamilies",
        "$value": "Roboto Mono"
      }
    },
    "lineHeights": {
      "h1": {
        "$type": "lineHeights",
        "$value": "110%"
      },
      "h2": {
        "$type": "lineHeights",
        "$value": "133%"
      },
      "h3": {
        "$type": "lineHeights",
        "$value": "160%"
      },
      "body": {
        "$type": "lineHeights",
        "$value": "175%"
      },
      "caption": {
        "$type": "lineHeights",
        "$value": "142%"
      }
    },
    "letterSpacing": {
      "default": {
        "$type": "letterSpacing",
        "$value": "0"
      },
      "increased": {
        "$type": "letterSpacing",
        "$value": "150%"
      },
      "decreased": {
        "$type": "letterSpacing",
        "$value": "-5%"
      }
    },
    "fontWeights": {
      "headingRegular": {
        "$type": "fontWeights",
        "$value": "Regular"
      },
      "headingBold": {
        "$type": "fontWeights",
        "$value": "Bold"
      },
      "bodyRegular": {
        "$type": "fontWeights",
        "$value": "Regular"
      },
      "bodyBold": {
        "$type": "fontWeights",
        "$value": "Bold"
      }
    },
    "fontSizes": {
      "h1": {
        "$type": "fontSizes",
        "$value": "2.25em"
      },
      "h2": {
        "$type": "fontSizes",
        "$value": "1.5em"
      },
      "h3": {
        "$type": "fontSizes",
        "$value": "1.25em"
      },
      "body": {
        "$type": "fontSizes",
        "$value": "1em"
      },
      "caption": {
        "$type": "fontSizes",
        "$value": ".875em"
      }
    },
    "borderWidth": {
      "sm": {
        "$type": "borderWidth",
        "$value": "2"
      },
      "md": {
        "$type": "borderWidth",
        "$value": "4"
      }
    }
  },
  "dark": {
    "fg": {
      "default": {
        "$type": "color",
        "$value": "{colors.white}"
      },
      "subtle": {
        "$type": "color",
        "$value": "{colors.zinc.600}"
      }
    },
    "bg": {
      "default": {
        "$type": "color",
        "$value": "{colors.zinc.900}"
      }
    },
    "accent": {
      "default": {
        "$type": "color",
        "$value": "{colors.teal.600}"
      },
      "subtle": {
        "$type": "color",
        "$value": "{colors.teal.300}"
      }
    }
  },
  "theme": {
    "button": {
      "background": {
        "$type": "color",
        "$value": "{bg.default}"
      },
      "text": {
        "$type": "color",
        "$value": "{fg.default}"
      },
      "border": {
        "$type": "border",
        "$value": {
          "width": "{borderWidth.sm}",
          "color": "{fg.default}",
          "style": "solid"
        }
      },
      "space": {
        "horizontal": {
          "$type": "spacing",
          "$value": "{spacing.xs}"
        },
        "vertical": {
          "$type": "spacing",
          "$value": "{spacing.xs}"
        }
      },
      "borderRadius": {
        "$type": "borderRadius",
        "$value": "{borderRadius.none}"
      }
    },
    "tag": {
      "active": {
        "text": {
          "$type": "color",
          "$value": "{accent.default}"
        },
        "border": {
          "$type": "border",
          "$value": {
            "width": "{borderWidth.sm}",
            "color": "{accent.default}",
            "style": "solid"
          }
        }
      },
      "background": {
        "$type": "color",
        "$value": "{bg.default}"
      },
      "text": {
        "$type": "color",
        "$value": "{fg.default}"
      },
      "font": {
        "$type": "typography",
        "$value": "{typography.body}"
      },
      "space": {
        "horizontal": {
          "$type": "spacing",
          "$value": "{spacing.xs}"
        },
        "vertical": {
          "$type": "spacing",
          "$value": "{spacing.xs}"
        }
      },
      "border": {
        "$type": "border",
        "$value": {
          "width": "{borderWidth.sm}",
          "color": "{fg.default}",
          "style": "solid"
        }
      },
      "borderRadius": {
        "$type": "borderRadius",
        "$value": "{borderRadius.none}"
      }
    },
    "link": {
      "internal": {
        "decoration": {
          "$type": "textDecoration",
          "$value": "underline"
        },
        "hover": {
          "underline": {
            "$type": "color",
            "$value": "{accent.subtle}"
          }
        }
      },
      "font": {
        "$type": "typography",
        "$value": "{typography.body}"
      },
      "text": {
        "$type": "color",
        "$value": "{accent.default}"
      }
    },
    "typography": {
      "heading": {
        "h1": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{fontFamilies.heading}",
            "fontWeight": "{fontWeights.headingBold}",
            "lineHeight": "{lineHeights.h1}",
            "fontSize": "{fontSizes.h1}",
            "letterSpacing": "{letterSpacing.decreased}"
          }
        },
        "h2": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{fontFamilies.heading}",
            "fontWeight": "{fontWeights.headingBold}",
            "lineHeight": "{lineHeights.h2}",
            "fontSize": "{fontSizes.h2}",
            "letterSpacing": "{letterSpacing.decreased}"
          }
        },
        "h3": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{fontFamilies.heading}",
            "fontWeight": "{fontWeights.headingBold}",
            "lineHeight": "{lineHeights.h3}",
            "fontSize": "{fontSizes.h3}",
            "letterSpacing": "{letterSpacing.decreased}"
          }
        }
      },
      "body": {
        "regular": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{fontFamilies.body}",
            "fontWeight": "{fontWeights.bodyRegular}",
            "lineHeight": "{lineHeights.body}",
            "fontSize": "{fontSizes.body}"
          }
        },
        "small": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{fontFamilies.body}",
            "fontWeight": "{fontWeights.bodyRegular}",
            "lineHeight": "{lineHeights.caption}",
            "fontSize": "{fontSizes.body}"
          }
        }
      },
      "code": {
        "regular": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{fontFamilies.code}",
            "fontWeight": "{fontWeights.bodyRegular}",
            "lineHeight": "{lineHeights.body}",
            "fontSize": "{fontSizes.body}"
          }
        }
      }
    },
    "box": {
      "background": {
        "$type": "color",
        "$value": "{bg.default}"
      },
      "border": {
        "$type": "border",
        "$value": {
          "width": "{borderWidth.sm}",
          "color": "{fg.default}",
          "style": "solid"
        }
      },
      "borderRadius": {
        "$type": "borderRadius",
        "$value": "{borderRadius.none}"
      },
      "spacing": {
        "$type": "spacing",
        "$value": "{spacing.sm}"
      }
    },
    "center": {
      "maxWidth": {
        "$type": "other",
        "$value": "80ch",
        "$description": "max-inline-size"
      },
      "gutters": {
        "$type": "spacing",
        "$value": "{spacing.md}"
      }
    },
    "cluster": {
      "gap": {
        "$type": "spacing",
        "$value": "{spacing.sm}"
      }
    },
    "stack": {
      "space": {
        "$type": "spacing",
        "$value": "{spacing.sm}"
      }
    },
    "icon": {
      "small": {
        "$type": "dimension",
        "$value": "16px"
      },
      "medium": {
        "$type": "dimension",
        "$value": "24px"
      },
      "large": {
        "$type": "dimension",
        "$value": "32px"
      }
    },
    "anchor": {
      "primary": {
        "opacity": {
          "$type": "opacity",
          "$value": "{opacity.none}"
        }
      },
      "hover": {
        "opacity": {
          "$type": "opacity",
          "$value": "{opacity.max}"
        }
      },
      "size": {
        "$type": "dimension",
        "$value": "16px"
      },
      "space": {
        "$type": "dimension",
        "$value": "0.25em"
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "core",
      "dark",
      "theme"
    ]
  }
}